{
	"compileOnSave": false,
	"strictNullChecks": true,
	"compilerOptions": {
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "declaration": true,
	  "removeComments": true,
	  "module": "commonjs",
	  "target": "es6",
	  "noImplicitAny": true,
	  "resolveJsonModule": true,
	  "types": [
		"node",
	  ],
	  "esModuleInterop": true,
	},
}  